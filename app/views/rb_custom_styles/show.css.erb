<%-  Tracker.find_all_by_id(Setting.plugin_redmine_backlogs[:story_trackers]).each do |tracker| %>

#backlogs_container .stories .story .id.tracker_<%= tracker.id %> .t * {
  background-color:#<%= Setting.plugin_redmine_backlogs[:custom_styles]["tracker_" + tracker.id]["background-color"] %>;
  color:#<%= Setting.plugin_redmine_backlogs[:custom_styles]["tracker_" + tracker.id]["color"] %>;
}

<%- end %>