<% colors = ["FF0000", "00FF00", "0000FF", "FFFFCC", "FCFCFC", "CFCFCF", "336699"] %>
<%-  Tracker.find_all_by_id(Setting.plugin_redmine_backlogs[:story_trackers]).each_with_index do |tracker, index| %>
#backlogs_container .stories .story.<%= tracker.name %> .id .t * {
  background-color:#<%= colors[index] %>;
}
<%- end %>